<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <comment>Querys relacionadas aos utilitarios</comment>
	
	<entry key="com.linecode.util.dao.UtilDao.getListaPerfil">
		SELECT 
			ID_ROLE,
			NOME
		FROM TAB_ROLE
	</entry>
	
	<entry key="com.linecode.util.dao.UtilDao.getListaEspecie">
		SELECT 
			ID_ESPECIE,
			NOME
		FROM TAB_ESPECIE
	</entry>
	
	<entry key="com.linecode.util.dao.UtilDao.getListaBioterio">
		SELECT 
			ID_BIOTERIO,
			NOME
		FROM TAB_BIOTERIO
	</entry>
    	
</properties>